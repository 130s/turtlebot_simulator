<launch>
  <!-- start gazebo with an empty plane -->
  <param name="/use_sim_time" value="true" />

  <node launch-prefix="optirun -f " name="gazebo" pkg="gazebo" type="gazebo" args="-u $(find gazebo_worlds)/worlds/empty.world" respawn="false" output="screen"/>


  <include file="$(find turtlebot_gazebo)/launch/robot.launch"/>
</launch>
